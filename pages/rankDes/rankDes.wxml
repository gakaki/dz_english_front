<view class="con-bg con">
  <scroll-view scroll-y class='scroll'>
    <view class="sc-box">
      <view wx:for="{{stage}}" wx:key="{{index}}" class="item f-r-fs">
        <image class="head" src="https://gengxin.odao.com/update/h5/yingyu/headframe/{{item.cfg.frame}}.png"></image>
        <text class="des">{{item.cfg.stage}}</text>
        <text class="jl">奖励:</text>
        <image src="{{awardIcon[index]}}" class="icon"></image>
        <text class="num">X{{item.cfg.award['v']}}</text>
      </view>
    </view>   
  </scroll-view>
  <view class="bot-bg flex">
     <text class="pre-season">段位框和奖励将在赛季结算时获得</text> 
  </view> 
</view>
