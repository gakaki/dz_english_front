<view class="duizhan">
  <view class="lan">
    <view class="lan-shouji">
      <view class="lan-ziliao">
        <image class="touxiangkuang" src="https://gengxin.odao.com/update/h5/yingyu/headframe/{{frameSelf}}.png" wx:if="{{frameSelf}}"></image>
        <image class="lan-touxiang" src="{{isSelf.info.avatarUrl}}"></image>
        <text class="lan-nicheng font">{{isSelf.info.nickName}}</text>
      </view>
    </view>
    <image class="animate animate-left" src="{{animation[index]}}"></image>
    <image class="zhejiao" src="https://gengxin.odao.com/update/h5/yingyu/duizhan/zhejiao.png"></image>
    <view class="lan-tankuang font">
      <view class="lan-liansheng" wx:if="{{isSelf.info.character.winningStreak}}">
        <text class="lan-liansheng-shuzi lan-xinxi">{{isSelf.info.character.winningStreak}}</text>
        <text class="lan-liansheng-wenzi">连胜</text>
      </view>
      <view class="lan-dizhi lan-xinxi" wx:if="{{isSelf.info.city}}">
        <image class="lan-dizhi-tu" src="https://gengxin.odao.com/update/h5/yingyu/duizhan/dizhi.png"></image>
        <text class="lan-dizhi-wenzi">{{isSelf.info.city}}</text>
      </view>
      <text class="lan-dengji lan-xinxi">LV.{{isSelf.info.character.level}}</text>
    </view>
  </view>
  <view class="pk">
    <view class="pk-lan">
      <!-- <view class="point"></view> -->
      <image src="https://gengxin.odao.com/update/h5/yingyu/duizhan/chatou-lan.png"></image>
    </view>
    <view class="pk-hong">
      <image src="https://gengxin.odao.com/update/h5/yingyu/duizhan/chatou-hong.png"></image>
    </view>
    <view class="animation-dianliu-lan {{yincang}}">
      <image class="animation-dianliuL" src="{{animation_dianliu_left[index_dianliu]}}"></image>
    </view>
    <view class="animation-dianliu-hong {{yincang}}">
      <image class="animation-dianliuR" src="{{animation_dianliu_right[index_dianliu]}}"></image>
    </view>
    <view class="shandian">
      <image class="dianliu" src="https://gengxin.odao.com/update/h5/yingyu/duizhan/dianliu.png"></image>
      <image class="dianliu2" src="https://gengxin.odao.com/update/h5/yingyu/duizhan/dianliu2.png"></image>
      <image class="dianliu3" src="https://gengxin.odao.com/update/h5/yingyu/duizhan/dianliu3.png"></image>
      <image class="dianliu4" src="https://gengxin.odao.com/update/h5/yingyu/duizhan/dianliu4.png"></image>
      <image class="dianliu5" src="https://gengxin.odao.com/update/h5/yingyu/duizhan/dianliu5.png"></image>
    </view>
    <view class="pk-donghua">
      <image class="pk-tu" src="https://gengxin.odao.com/update/h5/yingyu/duizhan/PK.png"></image>
      <image class="p-lizi" src="{{animation_p_lizi[index_p_lizi]}}"></image>
      <image class="k-lizi" src="{{animation_k_lizi[index_k_lizi]}}"></image>
    </view>
  </view>
  <view class="hong">
    <view class="hong-tankuang font">
      <view class="hong-liansheng" wx:if="{{notSelf.info.character.winningStreak}}">
        <text class="hong-liansheng-shuzi hong-xinxi">{{notSelf.info.character.winningStreak}}</text>
        <text class="hong-liansheng-wenzi">连胜</text>
      </view>
      <view class="hong-dizhi hong-xinxi" wx:if="{{notSelf.info.city}}">
        <image class="hong-dizhi-tu" src="https://gengxin.odao.com/update/h5/yingyu/duizhan/dizhi.png"></image>
        <text class="hong-dizhi-wenzi">{{notSelf.info.city}}</text>
      </view>
      <text class="hong-dengji hong-xinxi">LV.{{notSelf.info.character.level}}</text>
    </view>
    <image class="animate animate-right" src="{{animation[index]}}"></image>
    <image class="zhejiao-2" src="https://gengxin.odao.com/update/h5/yingyu/duizhan/zhejiao.png"></image>
    <view class="hong-shouji">
      <view class="hong-ziliao">
        <image class="touxiangkuang" src="https://gengxin.odao.com/update/h5/yingyu/headframe/{{frameOther}}.png" wx:if="{{frameOther}}"></image>
        <image class="hong-touxiang" src="{{notSelf.info.avatarUrl}}"></image>
        <text class="hong-nicheng font">{{notSelf.info.nickName}}</text>
      </view>
    </view>
  </view>
</view>