#rank {
  width: 100vw;
  height: 100vh;
   background: url('https://gengxin.odao.com/update/h5/yingyu/common/bg.jpg') 0 0 no-repeat;
  background-size: cover;
}
.tab {
  position: relative;
   width: 750rpx;
  height: 154rpx;
  background: url('https://gengxin.odao.com/update/h5/yingyu/rank/tab-bg.png') 0 0 no-repeat;
  background-size: 750rpx 154rpx;
}
.des {
  position: absolute;
  top: 19rpx;
  right: 20rpx;
   width: 44rpx;
  height: 44rpx;
}
.tab text {
 line-height: 72rpx;
  width: 212rpx;
  height: 72rpx;
  font-size: 30rpx;
  font-weight: bold;
  margin-top: 10rpx;
}
.tab-one {
  text-align: center;
  border-bottom-left-radius: 36rpx;
  border-top-left-radius: 36rpx;
  
}
.tab-two {
 text-align: center;
  border-bottom-right-radius: 36rpx;
  border-top-right-radius: 36rpx
}
.tab-act {
background-color: rgba(165,209,253,.8);
  text-decoration: underline;
  color: #335bb8
}
.tab-def {
 color: #fff;
  background-color: rgba(0,0,0,.2);
}
.item {
  align-items: center;
  position: relative;
  width: 709rpx;
  height: 145rpx;
  background: url('https://gengxin.odao.com/update/h5/yingyu/rank/item-bg.png') 0 0 no-repeat;
  background-size: cover;
}
.one {
position: absolute;
left: 5rpx;
top: -15rpx;
font-weight: bold;
font-size: 45rpx;
color: #fee01a;
-webkit-text-stroke-width: 4rpx;
  -webkit-text-stroke-color: #ff1414;
}
.head-box { 
  position: relative;
   background-color: #ffc024;
   width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
   margin-left: 32rpx;
}
.head {
  position: absolute;
  left: 4rpx;
  top: 4rpx;
  background-color: #335bb8;
   width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  box-sizing: border-box;
  border: 5rpx solid #fff;
}
.info {
  color: #b07145;
  font-size: 34rpx;
  margin-left: 14rpx;
  height: 120rpx;
  width: 520rpx;
}
.info-top,.info-bot {
  height: 60rpx;
  width: 520rpx;
  
}
.info-top {
  align-items: flex-start;
  border-bottom: 2rpx solid #ffe071;
}
.info-bot {
align-items: flex-end;
}
.gps-icon {
  margin-top: 9rpx;
   height: 35rpx;
  width: 27rpx;
}
.adress {
  margin-left: 5rpx;
}
.nn {
  flex-grow: 2
}
.star-box {
  align-items: center;
  margin-top: 9rpx;
  height: 53rpx;
  width: 164rpx;
  border-radius: 26rpx;
  background-color: #ffeeaf
}
.star-box image {
  margin-left: 10rpx;
}
.star-box text {
  margin-left: 9rpx;
}
.star-icon {
  height: 39rpx;
  width: 39rpx;
}