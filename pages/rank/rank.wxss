  bottom: 83rpx;
  overflow: hidden;
}

.item {
  margin: 0 auto;
  margin-bottom: 20rpx;
  align-items: center;
  position: relative;
  width: 690rpx;
  height: 150rpx;
  background-color: #fffef7;
  box-shadow: 0 3rpx 0 #f3e2c0;
  border-radius: 20rpx;
  z-index: 20;
}

.item:last-child {
  margin-bottom: 0;
}

.lt {
  position: absolute;
  left: 0;
  top: 0;
  width: 100rpx;
  height: 100rpx;
}

.one {
  z-index: 11;
