  top: 0;
  left: 0;
    width: 227rpx;
  height: 181rpx;  
  z-index: 6;
  transform: scale(0.8, 0.8);
}

.header {
  z-index: 5;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 120rpx;
