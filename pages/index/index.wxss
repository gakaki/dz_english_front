/**index.wxss**/

.container {
  width: 750rpx;
  height: 1334rpx;
  background: url('https://gengxin.odao.com/update/h5/yingyu/index/bg.jpg') 0 0 no-repeat;
  background-size: 750rpx 1334rpx;
  margin-top: 0rpx;
}
.title {
  display: block;
  margin-top: 64rpx;
  color: white;
}

.userInfo {
  position: relative;
  margin-top: 35rpx;
  width: 100%;
  height: 128rpx;
}

.header {
  width: 128rpx;
  height: 128rpx;
  box-sizing: border-box;
  border: 5rpx solid #0673d7;
  border-radius: 50%;
  margin: 0 14rpx 0 20rpx;
}
.userinfo-avatar {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  box-sizing: border-box;
  border: 3rpx solid #fff;
}
.info {
  height: 100%;
  color: #fff;
}

.nn {
  font-size: 32rpx;
  font-family: 'Microsoft YaHei';
}
.lvl {
  font-size: 28rpx;
  font-weight: bold;
  font-family: 'Microsoft YaHei';
  display: inline-block;
  height: 30rpx;
}
.lvl-box {
  position: relative;
  width: 180rpx;
  height: 30rpx;
  background-color: #09579f;
  margin-left: 10rpx;
  border-radius: 15rpx;
}
.lvl-box text {
  position: absolute;
  left: 50%;
  top: 0;
  transform: translateX(-50%);
  height: 30rpx;
  line-height: 30rpx;
  font-size: 24rpx;
  font-weight: bold;
  color: #fff;
}
.percent-box {
   width: 176rpx;
  height: 26rpx;
  margin: 2rpx;
  overflow: hidden
}
.percent-bg {
 width: 140rpx;
  height: 100%;
  background-color: #ffa800;
 border-radius: 13rpx;
}
.share {
  position: absolute;
  right: 32rpx;
  top: 5rpx;
  width: 44rpx;
  height: 44rpx;
}
.match-bg {
  margin-top: 88rpx;
   width: 712rpx;
  height: 267rpx;
}
.duizhan-bg {
  width: 712rpx;
  height: 281rpx;
}
.tab {
  flex-wrap: wrap;
  padding: 0 20rpx;
  margin-top: 39rpx;
}
.tab image {
  width: 220rpx;
  height: 165rpx;
  margin-bottom: 20rpx;
}