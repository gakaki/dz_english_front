.flex {
  display: flex;
  display: -webkit-flex;
  justify-content: center;
} 
.f-r-sb {
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
} 
.f-r-fs {
  display: flex;
  display: -webkit-flex;
  justify-content: flex-start;
}
.f-c-sb {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}
.f-c-sa {
    display: flex;
    flex-direction: column;
    justify-content: space-around;
}
.patl {
  position: absolute;
  left: 0;
  top: 0;
}
.patr {
  position: absolute;
  right: 0;
  top: 0;
}
.pabl {
  position: absolute;
  left: 0;
  bottom: 0;
}
.pabr {
  position: absolute;
  right: 0;
  bottom: 0;
}
.font{
  font-family: 'Microsoft YaHei';
  font-weight: bold;
  color: #fff;
}